<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="MainColor">White</Color>
    <Color x:Key="SecondaryColor">#c4cad4</Color>
    <Color x:Key="ComplementaryColor">#d5dff1</Color>
    <Color x:Key="SidebarBackground">#a3a9b4</Color>
    <Color x:Key="ListViewHederBackground">White</Color>
    <Color x:Key="MouseOverColor">#bad0f7</Color>
    <Color x:Key="MousePressedColor">#b2bed4</Color>
    <Color x:Key="MainForeground">Black</Color>

    <!--Border colors-->
    <Color x:Key="BorderLightColor">#FFCCCCCC</Color>
    <Color x:Key="BorderMediumColor">#FF888888</Color>
    <Color x:Key="BorderDarkColor">#FF444444</Color>



</ResourceDictionary>