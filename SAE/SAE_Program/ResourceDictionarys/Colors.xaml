<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="MainColor">White</Color>
    <Color x:Key="SecondaryColor">#c4cad4</Color>
    <Color x:Key="MainForeground">Black</Color>
    
    <Color x:Key="ComplementaryColor">#596880</Color>
    <Color x:Key="ComplementaryDisabledColor">#9BA1AB</Color>
    <Color x:Key="ComplementaryForeground">White</Color>
    
    <Color x:Key="SidebarBackground">#a3a9b4</Color>
    
    <Color x:Key="MouseOverColor">#384352</Color>
    <Color x:Key="MousePressedColor">#1F2633</Color>
</ResourceDictionary>